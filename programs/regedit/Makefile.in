	framewnd.c \
	hexedit.c \
	listview.c \
	main.c \
	regedit.c \
	regproc.c \
	treeview.c

RC_SRCS = regedit.rc

MANPAGES = regedit.man.in

SVG_SRCS = regedit.svg

INSTALL_LIB = regedit.exe $(wine64_disable)regedit
