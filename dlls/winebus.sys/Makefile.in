EXTRADEFS = -DWINE_NO_LONG_TYPES
MODULE    = winebus.sys
UNIXLIB   = winebus.so
IMPORTS   = ntoskrnl hidparse
EXTRALIBS = $(IOKIT_LIBS) $(UDEV_LIBS) $(PTHREAD_LIBS) $(INOTIFY_LIBS)
EXTRAINCL = $(UDEV_CFLAGS) $(SDL2_CFLAGS)

EXTRADLLFLAGS = -Wl,--subsystem,native

C_SRCS = \
	bus_iohid.c \
	bus_sdl.c \
	bus_udev.c \
	hid.c \
	main.c \
	unixlib.c

RC_SRCS = winebus.rc
