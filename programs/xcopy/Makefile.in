TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = xcopy.exe
APPMODE   = -mconsole
IMPORTS   = shell32 user32 msvcrt kernel32
EXTRADEFS = -DUNICODE
EXTRAINCL = -I$(TOPSRCDIR)/include/msvcrt
MODCFLAGS = @BUILTINFLAG@

C_SRCS = \
	xcopy.c

RC_SRCS = rsrc.rc

@MAKE_PROG_RULES@

@DEPENDENCIES@  # everything below this line is overwritten by make depend
