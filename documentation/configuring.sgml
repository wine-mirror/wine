                <entry>yes</entry>
                <entry>Sets up drives recognized by wine</entry>
              </row>
              <row>
                <entry>[wine]</entry>
                <entry>yes</entry>
                <entry>Settings for wine directories</entry>
              </row>
              <row>
                <entry>[DllDefaults]</entry>
