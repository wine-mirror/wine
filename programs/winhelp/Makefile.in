TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = winhelp.exe
APPMODE   = gui
IMPORTS   = comdlg32 shell32 user32 gdi32 kernel32
PROGRAMS  = hlp2sgml

C_SRCS = \
	winhelp.c \
	hlpfile.c \
	macro.c \
	string.c

EXTRA_SRCS = macro.lex.l
EXTRA_OBJS = @LEX_OUTPUT_ROOT@.o

EXTRAWRCFLAGS = -DWINELIB

RC_SRCS = rsrc.rc

@MAKE_PROG_RULES@

all: $(PROGRAMS)

hlp2sgml: hlp2sgml.o hlpfile.o
	$(CC) -o hlp2sgml hlp2sgml.o hlpfile.o

@LEX_OUTPUT_ROOT@.c: macro.lex.l
	$(LEX) -8 -i $(SRCDIR)/macro.lex.l

### Dependencies:
