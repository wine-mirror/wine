  WCMD_OVERWRITE, "Overwrite %s"
  WCMD_MORESTR, "Más..."
  WCMD_TRUNCATEDLINE, "Line in Batch processing possibly truncated. Using:\n"
  WCMD_NYI, "Not Yet Implemented\n\n"
