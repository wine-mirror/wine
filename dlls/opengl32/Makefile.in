MODULE    = opengl32.dll
UNIXLIB   = opengl32.so
EXTRADEFS = -D_OPENGL32_
IMPORTLIB = opengl32
IMPORTS   = user32 gdi32 advapi32 win32u
DELAYIMPORTS = glu32
UNIX_LIBS    = -lwin32u

EXTRADLLFLAGS = -Wl,--image-base,0x7a800000

VER_FILEDESCRIPTION_STR = "Wine OpenGL Client"
VER_PRODUCTVERSION      = 5,1,2600,2082
# CATIA needs 'Microsoft' in both CompanyName _and_ LegalCopyright
VER_LEGALCOPYRIGHT_STR  = "Copyright (c) 1993-2026 the Wine project. Not Microsoft"

SOURCES = \
	thunks.c \
	unix_thunks.c \
	unix_wgl.c \
	wgl.c
