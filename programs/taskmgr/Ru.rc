IDR_PROCESS_PAGE_CONTEXT MENU DISCARDABLE
BEGIN
    POPUP "DUMMY"
    BEGIN
        MENUITEM "&Завершить процесс",                          ID_PROCESS_PAGE_ENDPROCESS
