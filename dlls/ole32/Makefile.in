	memlockbytes.c \
	moniker.c \
	ole2.c \
	ole2stubs.c \
	ole2impl.c \
	ole32_main.c \
	oleobj.c \
	oleproxy.c \
	regsvr.c \
	rpc.c \
	stg_bigblockfile.c \
	stg_prop.c \
	stg_stream.c \
	storage32.c \
	stubmanager.c \
