TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = winetest.exe
APPMODE   = gui
IMPORTS   = comctl32 user32 wsock32

C_SRCS = \
	gui.c \
	main.c \
	send.c \
	util.c

RC_SRCS = \
	gui.rc \
	winetest.rc

RC_BINSRC = gui.rc
RC_BINARIES = wine.ico 

TESTS = \
	advapi32 \
	comctl32 \
	gdi32 \
	kernel32 \
	msvcrt \
	netapi32 \
	ntdll \
	oleaut32 \
	rpcrt4 \
	shell32 \
	shlwapi \
	urlmon \
	user32 \
	wininet \
	winmm \
	winspool.drv \
	ws2_32

@MAKE_PROG_RULES@

# Special rules

TESTBINS = $(TESTS:%=%_test.exe$(DLLEXT))

winetest.rc: maketest Makefile.in
	$(SRCDIR)/maketest $(TESTBINS) > $@ || ( $(RM) $@ && exit 1 )

winetest.res: $(TESTBINS)

clean::
	$(RM) winetest.rc $(TESTBINS)

depend: winetest.rc

# rules for stripping the test executables

advapi32_test.exe$(DLLEXT): $(DLLDIR)/advapi32/tests/advapi32_test.exe$(DLLEXT)
	cp $(DLLDIR)/advapi32/tests/advapi32_test.exe$(DLLEXT) $@ && $(STRIP) $@
