TOPSRCDIR = @top_srcdir@
TOPOBJDIR = ../..
SRCDIR    = @srcdir@
VPATH     = @srcdir@
MODULE    = wineboot.exe
APPMODE   = -mconsole
IMPORTS   = shell32 shlwapi version user32 advapi32 kernel32
EXTRALIBS = -luuid

C_SRCS = \
	shutdown.c \
	wineboot.c

RC_SRCS = wineboot.rc

@MAKE_PROG_RULES@

@DEPENDENCIES@  # everything below this line is overwritten by make depend
