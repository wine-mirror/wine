msgid "Success"
msgstr "Sucesso"

#: wldap32.rc:31
msgid "Operations Error"
msgstr "Erro de Operações"

#: wldap32.rc:32
msgid "Protocol Error"
msgstr "Erro de Protocolo"

#: wldap32.rc:33
msgid "Time Limit Exceeded"
msgstr "Tempo Limite Excedido"

#: wldap32.rc:34
msgid "Size Limit Exceeded"
msgstr "Tamanho Limite Excedido"

