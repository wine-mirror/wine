authors.c: $(top_srcdir)/AUTHORS
	(LC_ALL=C; export LC_ALL; echo 'const char * const SHELL_Authors[] = {' && \
	 sed -e '1,2d' -e 's/\(.*\)/  \"\1\",/' $(top_srcdir)/AUTHORS && \
	 echo '  0 };') >$@ || ($(RM) $@ && false)

